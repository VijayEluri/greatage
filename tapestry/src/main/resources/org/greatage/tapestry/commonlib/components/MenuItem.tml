<li t:id="border"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter">

	<t:delegate to="body"/>

	<t:block id="defaultBody">
		<t:if test="withLink">
			<a href="${model.link}">${model.label}</a>
			<p:else>
				<span>${model.label}</span>
			</p:else>
		</t:if>
	</t:block>
</li>