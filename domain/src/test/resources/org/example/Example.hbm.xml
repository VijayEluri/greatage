<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.example">

	<class name="Example" table="EXAMPLE" dynamic-insert="true" dynamic-update="true">

		<id name="id" column="ID" type="long" access="field">
			<generator class="increment"/>
		</id>
		<property name="deletedAt" column="DELETED" not-null="false"/>
		<!--<version name="version" access="field" column="VERSION"/>-->

		<property name="name" column="NAME" not-null="true"/>
	</class>

</hibernate-mapping>
