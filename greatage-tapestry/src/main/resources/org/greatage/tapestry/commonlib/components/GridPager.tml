<ul t:id="menu"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
	xmlns:p="tapestry:parameter">

	<t:loop source="pages" value="page" formState="none">
		<li t:id="menuItem">
			<t:if test="disabled">
				<span>${label}</span>
				<p:else>
					<a t:id="pageLink">${label}</a>
				</p:else>
			</t:if>
		</li>
	</t:loop>
</ul>
