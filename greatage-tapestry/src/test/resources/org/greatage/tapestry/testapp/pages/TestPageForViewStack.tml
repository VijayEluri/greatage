<html
		xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
		xmlns:c="tapestry-library:common"
		xmlns:p="tapestry:parameter">

<p>
	<span id="test1">
		<c:viewstack state="literal:null"/>
	</span>
	<span id="test2">
		<c:viewstack state="literal:null">
			<p:view>view</p:view>
		</c:viewstack>
	</span>
	<span id="test3">
		<c:viewstack state="literal:add">
			<p:view>view</p:view>
		</c:viewstack>
	</span>
	<span id="test4">
		<c:viewstack state="literal:view">
			<p:view>view</p:view>
		</c:viewstack>
	</span>
	<span id="test5">
		<c:viewstack state="literal:add">
			<p:view>view</p:view>
			add
		</c:viewstack>
	</span>
	<span id="test6">
		<c:viewstack state="literal:null">
			add
		</c:viewstack>
	</span>
	<span id="test7">
		<c:viewstack state="literal:edit">
			<p:view>view</p:view>
			<p:add>add</p:add>
			<p:edit>edit</p:edit>
			default
		</c:viewstack>
	</span>
	<span id="test8">
		<c:viewstack state="literal:default">
			<p:view>view</p:view>
			<p:add>add</p:add>
			<p:edit>edit</p:edit>
			default
		</c:viewstack>
	</span>
</p>
</html>